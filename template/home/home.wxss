.homeWrap{
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: #d3f3f3;
}
.homeWrap .homeTop{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 100%;
  height: 60rpx;
  background: red;
}
.homeWrap .homeCtn{
  margin: auto;
  padding: 60rpx 0;
  width: 680rpx;
  height: 100%;
  box-sizing: border-box;
}
.homeWrap .homeCtn .musicBox{
  width: 100%;
  height: 100%;
  border-radius: 16rpx;
  background: #FFF;
  box-shadow: 0 0 10rpx #ececec;
}
.homeWrap .homeBtm{
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 100%;
  height: 60rpx;
  background: yellow;
}