/**index.wxss**/
@import '../../template/home/home.wxss';
page{
  width: 100%;
  height: 100%;
}
.wrap{
  width: 100%;
  height: 100%;
}
.loginPop{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.6);
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center; 
}
.loginPop .loginPopCtn{
  width: 80%;
}
.loginPop .loginText{
  padding: 20rpx;
  background: #FFF;
  text-align: center;

}
.loginPop .loginButton{
  display: flex;
  flex-direction: row;
  width: 100%;
}
.loginPop .loginButton .loginBtnBox{
  width: 100%;
  border-top: 1rpx solid #ececec;
  background: #FFF;
}
.loginPop .loginButton .loginBtnBox:nth-child(1){
  border-right: 1rpx solid #ececec;

}
.loginPop .loginButton .loginBtnBox .loginBtn{
  width: 100%;
  border: none;
}
